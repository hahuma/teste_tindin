{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "rules": {
        "prettier/prettier": "error",
        "no-underscore-dangle": "off",
        "class-methods-use-this": "off",
        "camelcase": "off",
        "global-require": "off"
    }
}
